{
  "name": "agblogger-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc -b --noEmit",
    "lint": "eslint .",
    "lint:deps": "dependency-cruiser --config .dependency-cruiser.cjs src",
    "lint:unused": "knip --config knip.json --include dependencies,unlisted,unresolved",
    "audit": "npm audit --audit-level=high --omit=dev",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "mutation": "stryker run stryker.mutation.config.mjs",
    "mutation:full": "stryker run stryker.mutation-full.config.mjs",
    "preview": "vite preview",
    "check": "npm run typecheck && npm run lint && npm run test"
  },
  "dependencies": {
    "@dagrejs/dagre": "^2.0.4",
    "@fontsource-variable/dm-sans": "^5.2.8",
    "@fontsource-variable/jetbrains-mono": "^5.2.8",
    "@fontsource-variable/lora": "^5.2.8",
    "@xyflow/react": "^12.0.0",
    "date-fns": "^4.1.0",
    "katex": "^0.16.0",
    "ky": "^1.8.0",
    "lucide-react": "^0.511.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.6.0",
    "zustand": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@stryker-mutator/core": "9.5.1",
    "@stryker-mutator/typescript-checker": "9.5.1",
    "@stryker-mutator/vitest-runner": "9.5.1",
    "@tailwindcss/vite": "^4.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/katex": "^0.16.0",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.18",
    "dependency-cruiser": "^17.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "fast-check": "^4.5.3",
    "globals": "^16.5.0",
    "jsdom": "^28.1.0",
    "knip": "^5.0.0",
    "tailwindcss": "^4.1.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "minimatch": "^10.2.1",
    "@stryker-mutator/core": {
      "ajv": "8.18.0"
    }
  }
}
